<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="-1dp"
        android:right="-1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <size
                android:width="200dp"
                android:height="8dp" />
            <solid android:color="@color/white" />
            <stroke
                android:width="0.5dp"
                android:color="@color/ce1e1e1" />
        </shape>
    </item>
</layer-list>