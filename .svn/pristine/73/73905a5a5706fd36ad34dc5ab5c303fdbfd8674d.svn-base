<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.realpower.petitionwatch.modelcounty.activity.MyTaskDetailActivity">

    <include layout="@layout/activitytitle_common" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusableInTouchMode="true"
        android:orientation="vertical"
        android:padding="@dimen/pagePadding">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="任务时间"
            android:theme="@style/PageLineTitleTextView" />

        <TextView
            android:id="@+id/tv_time"
            style="@style/PageLineContentTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="20183255" />

        <TextView
            style="@style/PageLineTitleTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="任务内容" />

        <TextView
            android:id="@+id/tv_content"
            style="@style/PageLineContentTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


        <TextView
            style="@style/PageLineTitleTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="任务领导" />


        <TextView
            android:id="@+id/tv_from"
            style="@style/PageLineContentTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />


    </LinearLayout>
</LinearLayout>
