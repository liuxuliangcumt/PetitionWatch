package com.realpower.petitionwatch.net.param;

import java.util.List;

/**
 * Created by Administrator on 2018/1/18.
 */

public class DealParam {

    private List<String> criterias;


    public DealParam(String criteria, String id) {
        this.criteria = criteria;
        this.id = id;
    }

    public DealParam(List<String> criterias, String criteria) {
        this.criterias = criterias;
        this.criteria = criteria;
    }

    public DealParam(List<String> criterias) {
        this.criterias = criterias;
    }

    /**
     * criteria : string
     * criterias : ["string"]
     * id : string
     */

    private String criteria;
    private String id;

    public String getCriteria() {
        return criteria;
    }

    public void setCriteria(String criteria) {
        this.criteria = criteria;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public List<String> getCriterias() {
        return criterias;
    }

    public void setCriterias(List<String> criterias) {
        this.criterias = criterias;
    }
}
